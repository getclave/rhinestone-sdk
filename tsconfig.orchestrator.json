{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": false,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/orchestrator"],
  "exclude": ["src/examples"]
}
